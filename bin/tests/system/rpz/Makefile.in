srcdir =	@srcdir@
VPATH =		@srcdir@
top_srcdir =	@top_srcdir@

VERSION=@BIND9_VERSION@

@BIND9_MAKE_INCLUDES@

CINCLUDES =     ${ISC_INCLUDES} ${DNS_INCLUDES}

CDEFINES =
CWARNINGS =

DNSLIBS =
ISCLIBS =	../../../../lib/isc/libisc.@A@

DNSDEPLIBS =
ISCDEPLIBS =    ../../../../lib/isc/libisc.@A@

DEPLIBS =	${ISCDEPLIBS}

LIBS =		${ISCLIBS} @LIBS@

TARGETS =	fastrpz@EXEEXT@

FASTRPZOBJS =	fastrpz.@O@

SRCS =		fastrpz.c

@BIND9_MAKE_RULES@

all: fastrpz@EXEEXT@

fastrpz@EXEEXT@: ${FASTRPZOBJS} ${DEPLIBS}
	${LIBTOOL_MODE_LINK} ${PURIFY} ${CC} ${CFLAGS} ${LDFLAGS} -o $@ ${FASTRPZOBJS} ${LIBS}

clean distclean::
	rm -f ${TARGETS}

